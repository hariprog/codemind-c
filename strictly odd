#include<stdio.h>
int main() {
    int x,count=0,c=0;
    scanf("%d",&x);
    int a[x];
    for(int i=0;i<x;i++){
        scanf("%d",&a[x]);
    }
    for(int j=0;j<x;j+=2){
        count++;
        if(a[j+1]%2!=0) {
        c++;
        }
    }
    if(count==c)
    printf("True");
    else 
    printf("False");
}
